{
  "name": "copyrighter",
  "version": "1.2.2",
  "description": "Copyright Text Automation Tool",
  "main": "./bin/copyrighter.ts",
  "scripts": {
    "test": "tsc --project tsconfig.test.json && jest --testPathPattern=dist-tests/tests/",
    "test:silent": "tsc --project tsconfig.test.json && jest --silent --testPathPattern=dist-tests/tests/",
    "build": "tsc",
    "start": "tsc && node dist/index.js"
  },
  "keywords": [
    "copyright"
  ],
  "bin": {
    "copyrighter": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://dsvictorh/copyrighter.git"
  },
  "author": "Victor Herrera",
  "dependencies": {
    "yargs": "^16.2.0",
    "@types/node": "^14.14.41",
    "@types/yargs": "^16.0.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3"
  }
}
