{
    "copyright": "Copyright (c) {year} Corporation, Inc.\n\nThe right to copy, distribute, modify, or otherwise make use of this software may be licensed only pursuant to the terms of an applicable Wind River license agreement.",
    "regex": "Copyright \\(c\\) {year} Corporation, Inc\\.\\n\\nThe right to copy, distribute, modify, or otherwise make use of this software may be licensed only pursuant to the terms of an applicable Wind River license agreement\\.",
    "defaultExcludedFolders": ["node_modules", "dist", ".git", "dist-tests", "bin"],
    "extensions": {
        "html": {
            "prefixTexts": ["<!DOCTYPE html>"],
            "startComment": "<!--",
            "endComment": "-->"
        },
        "css": {
            "startComment": "/*",
            "startCommentRegex": "\\/\\*",
            "endComment": "*/",
            "endCommentRegex": "\\*\\/"
        },
        "scss": {
            "startComment": "/*",
            "startCommentRegex": "\\/\\*",
            "endComment": "*/",
            "endCommentRegex": "\\*\\/"
        },
        "js": {
            "prefixTexts": ["#!/usr/bin/env node"],
            "startComment": "/*",
            "startCommentRegex": "\\/\\*",
            "endComment": "*/",
            "endCommentRegex": "\\*\\/"
        },
        "ts": {
            "prefixTexts": ["#!/usr/bin/env node"],
            "startComment": "/*",
            "startCommentRegex": "\\/\\*",
            "endComment": "*/",
            "endCommentRegex": "\\*\\/"
        },
        "jenkinsfile": {
            "startComment": "/*",
            "startCommentRegex": "\\/\\*",
            "endComment": "*/",
            "endCommentRegex": "\\*\\/"
        },
        "dockerfile": {
            "comment": "#"
        },
        "sh": {
            "prefixTexts": ["#!/bin/sh", "#!/bin/sh -e","#!/bin/bash", "#!/bin/bash -e", "#!/usr/bin/env bash"],
            "comment": "#"
        },
        "gitignore": {
            "comment": "#"
        },
        "py": {
            "prefixTexts": ["#!/usr/bin/env python3"],
            "comment": "#"
        },
        "xml": {
            "startComment": "<!--",
            "endComment": "-->"
        },
        "sql": {
            "comment": "--"
        },
        "tpl": {
            "comment": "#"
        },
        "tf": {
            "comment": "#"
        },
        "yaml": {
            "comment": "#"
        }
    }
}